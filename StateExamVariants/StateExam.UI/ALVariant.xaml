        <Button x:Name="Task7button" Content="7" Grid.Row="6" Grid.Column="0" Click="Task7button_Click"/>
        <Button x:Name="Task8button" Content="8" Grid.Row="0" Grid.Column="1" Click="Task8button_Click"/>
        <Button x:Name="Task9button" Content="9" Grid.Row="1" Grid.Column="1" Click="Task9button_Click"/>
        <Button x:Name="Task10button" Content="10" Grid.Row="2" Grid.Column="1" Click="Task10button_Click"/>
        <Button x:Name="Task11button" Content="11" Grid.Row="3" Grid.Column="1" Click="Task11button_Click"/>
