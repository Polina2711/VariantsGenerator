        <Button x:Name="Task3button" Content="3" Grid.Row="2" Grid.Column="0" Click="Task3button_Click"/>
        <Button x:Name="Task4button" Content="4" Grid.Row="3" Grid.Column="0" Click="Task4button_Click"/>
